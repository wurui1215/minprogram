page{
  background-color: #eee;
}
.search{
  width: 750rpx;
  height: 100rpx;
  background-color: #ffa754;
}
.search icon{
  width: 150rpx;
  height: 75rpx;
  background-color: #fff;
  margin-left: 10rpx;
  float: left;
  border-radius: 50rpx;
  margin-right: -20rpx;
}
.search input{
  width: 550rpx;
  height: 75rpx;
  background-color: #fff;
  border-radius: 50rpx;
  margin-left: -25px;
  float: left;
}
.search image{
  width: 50rpx;
  height: 50rpx;
  float: right;
  margin-right: 20rpx;
}
.search text{
  font-size: 12px;
  float: right;
  margin-right: 20rpx;
}

